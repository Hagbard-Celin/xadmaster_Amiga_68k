# TabSize 4
# $Id: Makefile,v 1.6 2004/01/25 15:29:56 andrewbell Exp $
#
# If you add new includes or change dependencies then
# remember to run the following command:
#
# mkdepend -I AutoGeneratedFiles/ -p .c:%n.sasc.o
#
# Make sure you run the following makefile rules for first time builds:
#
# > make proto
# > make classinfo
#
# So that mkdepend can find the auto generated sources!
#
# Usage:
#
# ------------------------------------------------------------------------------
# make clean     - Remove all objects and auto-generated files.
# ------------------------------------------------------------------------------
# make protos    - Build prototype files (in AutoGeneratedFiles).
#                  If you modify any line starting with GPROTO (global) or
#                  LPROTO (local - was never fully implemented in XProto), which
#                  indicates a function, you must build your protos so that the
#                  other sources can see your changes. Note that a lot of
#                  sources depend on the automatically generated protos, so if
#                  you recreate them then the compiler will rebuild all sources
#                  that need them.
# ------------------------------------------------------------------------------
# make classinfo - Update dynamically generated class headers.
#                  (in AutoGeneratedFiles)
#                  You MUST call this if you add or change any OVERLOAD()
#                  DECLARE(), EXPORT or CLASSDATA information in the
#                  CLASS_#?.c sources!
#
#                  For example, if you rename a method name:
#                    OLD: OVERLOAD(Function)
#                    NEW: OVERLOAD(RenamedFunction)
#
#                  Or if you add or change new parameters to an OVERLOAD()
#                  or DECLARE(). e.g.:
#                    OLD: DECLARE(UpdateProgress) // UBYTE *fileName
#                    NEW: OVERLOAD(UpdateProgress) // UBYTE *fileName, ULONG size
#
#                  Also if you change the contents of a CLASSDATA or EXPORT block.
#                  e.g.:
#                  OLD
#                  /* EXPORT
#                     #define XYZ 123
#                  */
#
#                  NEW:
#                  /* EXPORT
#                     #define XYZ 123
#                     #define ABC 321
#                  */
# ------------------------------------------------------------------------------
# Other defines you might want to added to GLOBAL_CFLAGS.
# (i.e use -DENABLE_MUIASLHACK on gcc or DEFINE=ENABLE_MUIASLHACK on sc)
#
# ENABLE_MUIASLHACK: Enables a small MUI hack which helps center off ASL
#                    requesters.
#

DATE               = Y2004
BACKUP_NAME        = $(DATE)_VX_Backup.lzx
BACKUP_ARC_PATH    = WinDH_C:Backups
BACKUP_ARC         = $(BACKUP_ARC_PATH)/$(BACKUP_NAME)
BACKUP_ARC_RAM     = Ram:$(BACKUP_NAME)
AS                 = phxass
AS_FLAGS           = OPT=3 SYMDEBUG LINEDEBUG QUIET
ON                 = *e[32m
OFF                = *e[0m
CC                 = sc
VX_TOOL            = vx_sasc
VX_AUTODIR         = AutoGeneratedFiles
VX_CSRCS           = vx_arc.c vx_cfg.c vx_debug.c vx_global.c vx_io.c vx_ipc.c \
                     vx_logo.c vx_main.c vx_mem.c vx_misc.c vx_virus.c mybrush.c
VX_CHDRS           = $(VX_CSRCS:.c=.h)
VX_COBJS           = $(VX_CSRCS:.c=.sasc.o)
VX_XPROTO_INPUTS   = vx_arc.c vx_cfg.c vx_debug.c vx_io.c vx_ipc.c vx_main.c \
                     vx_mem.c vx_misc.c vx_virus.c
VX_XPROTO_OUTPUTS  = $(subst vx_,$(VX_AUTODIR)/vx_,$(subst .c,_protos.h,$(VX_XPROTO_INPUTS)))
VX_EXTRHDRS        = vx_allHeaders.h vx_compilerDefs.h vx_include.h vx_muiSupport.h \
                     vx_rev.h CLASS_Common.h
VX_CLASS_SOURCES=    CLASS_AboutWin.c CLASS_ArcHistory.c CLASS_ArcInfoWin.c \
                     CLASS_ArcView.c CLASS_avmExplorer.c CLASS_avmFilesAndDirs.c \
                     CLASS_avmLinear.c CLASS_avmListtree.c CLASS_avmNListtree.c \
                     CLASS_BusyWin.c CLASS_CfgPageFT.c CLASS_CfgPageMain.c \
                     CLASS_CfgPageTB.c CLASS_CfgPageXAD.c CLASS_ErrorWin.c \
                     CLASS_ExtFileInfoWin.c CLASS_FileNameSearch.c \
                     CLASS_GetPatternWin.c CLASS_GetStringWin.c CLASS_MainWin.c \
                     CLASS_MultiPartWin.c CLASS_NewFileTypeWin.c CLASS_PortionList.c \
                     CLASS_ProgressWin.c CLASS_PsychedelicLamp.c CLASS_RecentMenu.c \
                     CLASS_SearchWin.c CLASS_SettingsWin.c CLASS_StringSearch.c \
                     CLASS_VX.c CLASS_VXToolBar.c CLASS_WriteDisk.c CLASS_XADInfoWin.c
VX_CLASS_OBJS      = $(VX_CLASS_SOURCES:.c=.sasc.o)
VX_GC_INPUTS       = $(VX_CLASS_SOURCES)
VX_GC_OUTPUTS      = $(subst CLASS_,$(VX_AUTODIR)/,$(subst .c,.h,$(VX_GC_INPUTS)))
GLOBAL_CFLAGS      = NOVER CPU=68020 UNSIGNEDCHAR SMALLCODE IDLEN=65 \
                     DATA=NEAR CODE=NEAR NOSTKCHK STRINGMERGE MEMORYSIZE=HUGE DEBUG=symbol \
                     IDIR=$(VX_AUTODIR) DEFINE=ENABLE_MUIASLHACK
VX_ASM_SRCS        = vx_asm.s vx_mem_asm.s
VX_ASM_OBJS        = $(VX_ASM_SRCS:.s=.o)
VX_LIBS=

#------------------------------------------------------------------------------

XP_TOOL      = vxxp_sasc
XP_ALLSRCS   = xp_main.c xp_main.h xp_protos.h xp_rev.h
XP_OBJS      = xp_main.sasc.o
XP_CFLAGS    = $(GLOBAL_CFLAGS)

#------------------------------------------------------------------------------

GC_TOOL     = vxgc_sasc
GC_ALLSRCS  = gc_main.c gc_main.h gc_lists.c gc_lists.h
GC_OBJS     = gc_main.sasc.o gc_lists.sasc.o
GC_CFLAGS   = $(GLOBAL_CFLAGS)

#------------------------------------------------------------------------------
# VX rules
#------------------------------------------------------------------------------

$(VX_TOOL) : bulk.sasc.o $(VX_AUTODIR)/Classes.sasc.o
	@echo "$(ON)Linking                    --> $@$(OFF)"
	$(CC) LINK bulk.sasc.o $(GLOBAL_CFLAGS) $(VX_AUTODIR)/Classes.sasc.o TO $(VX_TOOL) $(VX_LIBS)
	@list $(VX_TOOL) LFORMAT="$(ON)Finished. Executable size is %l bytes.$(OFF)"
	@version $(VX_TOOL) FULL

$(VX_AUTODIR)/Classes.sasc.o : $(VX_AUTODIR)/Classes.c $(VX_AUTODIR)/Classes.h
	@echo "$(ON)Building class dispatchers $@ from $<$(OFF)"
	$(CC) $< OBJNAME=$@ $(GLOBAL_CFLAGS)

vx_gst.gst : vx_gst.h vx_gst.c
	@echo "$(ON)Building GST   --> $<$(OFF)"
	@gst unload $@
	@sc $(CFLAGS) nogst makegst=$@ noobjname vx_gst.c

$(VX_COBJS) : %.sasc.o : %.c
	@echo "$(ON)Building $@ from $<$(OFF)"
	$(CC) $< OBJNAME=$@ $(GLOBAL_CFLAGS)

$(VX_CLASS_OBJS) : %.sasc.o : %.c
	@echo "$(ON)Building class $@ from $<$(OFF)"
	$(CC) $< OBJNAME=$@ $(GLOBAL_CFLAGS)

$(VX_ASM_OBJS) : %.o : %.s
	@echo "$(ON)Assembling VX module --> $<$(OFF)"
	$(AS) $< TO $@ $(AS_FLAGS)

# This is needed because the command line gets too long
bulk.sasc.o : $(VX_ASM_OBJS) $(VX_COBJS) $(VX_CLASS_OBJS)
	@join mybrush.sasc.o $(VX_ASM_OBJS) (CLASS|vx)_#?.sasc.o AS $@

#------------------------------------------------------------------------------
# XProto (xp.exe) rules
#------------------------------------------------------------------------------

# The portable version of XProto borrows GC's list functions
$(XP_TOOL) : $(XP_OBJS) gc_lists.sasc.o
	@echo "$(ON)Linking XP tool --> $<$(OFF)"
	$(CC) link $(XP_CFLAGS) $(XP_OBJS) gc_lists.sasc.o TO $(XP_TOOL)

$(XP_OBJS) : $(XP_ALLSRCS)

#------------------------------------------------------------------------------
# GenClasses (gc.exe) rules
#------------------------------------------------------------------------------

$(GC_TOOL) : $(GC_OBJS)
	@echo "$(ON)Linking GC tool   --> $<$(OFF)"
	$(CC) link $(GC_CFLAGS) $(GC_OBJS) TO $(GC_TOOL)

$(GC_OBJS) : $(GC_ALLSRCS)

#------------------------------------------------------------------------------
# Utility rules
#------------------------------------------------------------------------------

.PHONY: showvars clean classinfo backup relsrc protos

clean:
	delete $(VX_TOOL) $(XP_TOOL) $(GC_TOOL) bulk.sasc.o QUIET
	delete #?.sasc.o QUIET
	delete $(VX_AUTODIR)/#? QUIET
	delete .pseudo_target QUIET

#	@delete $(VX_AUTODIR) QUIET

backup:
	echo "$(ON)Backing up sources to:*n$(BACKUP_ARC_RAM) and *n$(BACKUP_ARC)...$(OFF)" @@ \
	if EXISTS $(BACKUP_ARC_RAM) @@ \
	  delete $(BACKUP_ARC_RAM) FORCE QUIET @@ \
	endif @@ \
	if EXISTS $(BACKUP_ARC) @@ \
	  delete $(BACKUP_ARC) FORCE QUIET @@ \
	endif @@ \
	lzx -M5000 -1 -F a $(BACKUP_ARC_RAM) #?makefile#? #?.(c|h|txt|doc|readme) @@ \
	if EXISTS $(BACKUP_ARC_PATH) @@ \
	  copy $(BACKUP_ARC_RAM) $(BACKUP_ARC) @@ \
	endif @@ \
	list $(BACKUP_ARC_RAM) LFORMAT="$(ON)Backup finished. Archive size is %l bytes.$(OFF)"

relsrc:
	if EXISTS Ram:vx2 @@ \
	  delete Ram:vx2/Images/#? FORCE QUIET @@ \
	  delete Ram:vx2/Icons/#? FORCE QUIET @@ \
	  delete Ram:vx2/AutoGeneratedFiles/#? FORCE QUIET @@ \
	  delete Ram:vx2/#? FORCE QUIET @@ \
	  delete Ram:vx2 FORCE QUIET @@ \
	endif @@ \
	copy Makefile #?.(c|h|s|i) ram:vx2 @@ \
	copy Images ram:vx2/Images @@ \
	copy Icons ram:vx2/Icons @@ \
	if EXISTS Ram:VX2src.lha @@ \
	  delete Ram:VX2src.lha FORCE QUIET @@ \
	endif @@ \
	lha -F a -r Ram:VX2src.lha ram:vx2

protos : $(XP_TOOL)

protos:
	@if NOT EXISTS $(VX_AUTODIR) @@ \
	  makedir $(VX_AUTODIR) @@ \
	endif @@ \
	echo "$(ON)Generating proto file from $(VX_XPROTO_INPUTS)*nSaving to $(VX_AUTODIR)$(OFF)" @@ \
	$(XP_TOOL) $(VX_XPROTO_INPUTS) --protodir $(VX_AUTODIR)


classinfo : $(GC_TOOL)

classinfo:
	if NOT EXISTS $(VX_AUTODIR) @@ \
	  makedir $(VX_AUTODIR) @@ \
	endif @@ \
	$(GC_TOOL) -bVX "" -matchprefixCLASS_ -destdir$(VX_AUTODIR) -i"CLASS_Common.h","vx_muiSupport.h"


