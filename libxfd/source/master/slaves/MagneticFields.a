RB_Magnetic	tst.b	2(a0)
		bne.s	.Exit

		cmp.w	#'SC',(a0)	;12+4+4
		beq.s	.SC
		cmp.w	#'SP',(a0)	;12+4
		bne.s	.NoSP
.SC		;tst.b	2(a0)		;depth
		;bne.s	.Exit
		tst.b	4(a0)		;uncr
		bne.s	.Exit
.OldSA		tst.b	8(a0)		;cr
		bne.s	.Exit
		moveq	#1,d0
		rts

.NoSP		cmp.w	#'SF',(a0)	;10+4
		bne.s	.NoSF
		;tst.b	2(a0)		;uncr
		;bne.s	.Exit
		tst.b	6(a0)		;cr
		bne.s	.Exit
		moveq	#1,d0
		rts

.NoSF		cmp.w	#'SQ',(a0)	;10+4
		bne.s	.NoSQ
		;tst.b	2(a0)		;uncr
		;bne.s	.Exit
		move.b	6(a0),d0	;1-3
		beq.s	.Exit
		subq.b	#3,d0
		bgt.s	.Exit
		moveq	#1,d0
		rts

.NoSQ		cmp.w	#'SA',(a0)	;14+8+4
		bne.s	.Exit
		;tst.b	2(a0)		;depth
		;bne.s	.Exit
		tst.b	4(a0)		;special
		bne.s	.Exit
		tst.b	6(a0)		;uncr
		bne.s	.OldSA
		tst.b	10(a0)		;cr
		bne.s	.Exit
		moveq	#1,d0
		rts

.Exit		moveq	#0,d0
		rts

