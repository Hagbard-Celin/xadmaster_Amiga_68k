.OldSA		moveq	#12+8,d1
		add.l	8(a0),d1	;cr
		cmp.l	d1,a1
		bcs.s	.Exit
		add.l	(a0,d1.l),d1
		bra.s	.Test

.NoSA		cmp.w	#'SC',(a0)
		bne.s	.NoSC
		moveq	#2,d1
		move.w	2(a0),d0	;depth
		lsl.l	d0,d1
		add.l	#12,d1
		add.l	8(a0),d1	;cr
		bra.s	.Test

.NoSC		cmp.w	#'SP',(a0)
		bne.s	.NoSP
		moveq	#12,d1
		add.l	8(a0),d1	;cr
		bra.s	.Test

.NoSP		cmp.w	#'SF',(a0)
		bne.s	.NoSF
		moveq	#10,d1
		add.l	6(a0),d1	;cr
		bra.s	.Test

.NoSF		cmp.w	#'SQ',(a0)
		bne.s	.Exit
		moveq	#10,d1
		move.l	6(a0),d0	;cr
		and.l	#$ffffff,d0
		lsl.l	#2,d0
		add.l	d0,d1
.Test		cmp.l	d1,a1
		bcs.s	.Exit
		move.l	d1,d0
		rts

.Exit		moveq	#0,d0
		rts

